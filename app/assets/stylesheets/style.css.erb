/* ===== @work ===== */
 
#options {
  margin: 45px 0 0 0px;
}
#options ul {
  margin: 0;
  list-style: none;
}
#options ul ul {
  margin-left: 1.5em;
}
#options li {
  display: block;
  float: left;
  margin-bottom: 0.2em;
  margin-right: 0.2em;
}
#options li a {
  display: block;
  padding: 0.4em 0.5em;
}
.tag-css {
  padding-top: 5px;
  padding-bottom: 20px;
}
.tag-css li, .tag-css a {
    float: left;
    font-size: 11px;
    height: 24px;
    line-height: 14px;
    position: relative;
}
.tag-css a {
    background: none repeat scroll 0 0 #4E4E4E;
    border-bottom-right-radius: 4px;
    border-top-right-radius: 4px;
    color: #FFFFFF;
    height: 16px;
    margin-left: 20px;
    padding: 0 10px 0 12px;
    text-decoration: none;
    font-weight: bold;
}
.tag-css a:before {
    border-color: transparent #4E4E4E transparent transparent;
    border-style: solid;
    border-width: 12px 12px 12px 0;
    content: "";
    float: left;
    height: 0;
    left: -12px;
    position: absolute;
    top: 0;
    width: 0;
}
.tag-css a:after {
    background: none repeat scroll 0 0 #FFFFFF;
    border-radius: 2px 2px 2px 2px;
    box-shadow: -1px -1px 2px #004977;
    content: "";
    float: left;
    height: 4px;
    left: -2px;
    position: absolute;
    top: 10px;
    width: 4px;
}
.tag-css a:hover {
    background: none repeat scroll 0 0 #FDB826;
    color: #4E4E4E;
}
.tag-css a:hover:before {
    border-color: transparent #FDB826 transparent transparent;
    color: #4E4E4E;
}
.tag-css a:active {
    background: none repeat scroll 0 0 #eea202;
    color: #4E4E4E;
}
.tag-css a:active:before {
    border-color: transparent #eea202 transparent transparent;
    color: #4E4E4E;
}
.image-overlay-link, .image-overlay-zoom {
    background: none no-repeat scroll 50% center rgba(0, 0, 0, 0.4);
    cursor: pointer;
    display: none;
    height: 100%;
    left: 0;
